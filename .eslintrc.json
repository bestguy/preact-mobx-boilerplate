{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "import/extensions": 0,
    "object-curly-newline": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/require-default-props": 0,
    "react/sort-comp": [1, {
      "order": [
        "static-methods",
        "everything-else",
        "lifecycle",
        "render"
      ]
    }]
  }
}
